{
  "name": "backend",
  "packageManager": "yarn@4.9.2",
  "version": "0.0.1",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsup src/server.ts --dts --format esm,cjs --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migrate": "knex migrate:latest --knexfile knexfile.cjs",
    "seed": "knex seed:run --knexfile knexfile.cjs",
    "test": "vitest --run",
    "lint": "echo \"(backend) no lint rules yet\""
  },
  "dependencies": {
    "common": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.0.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "vitest": "^2.1.3"
  }
}
